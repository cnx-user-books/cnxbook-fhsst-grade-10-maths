                      <image mime-type="image/png" src="../../media/MG10C15_027.png" id="id88196_onlineimage" width="72"><!-- NOTE: attribute width changes image size online (pixels). original width is 72. --></image>
                      <image mime-type="application/postscript" for="pdf" src="../../media/MG10C15_027.eps" id="id88196_printimage"/>
                    </media>
