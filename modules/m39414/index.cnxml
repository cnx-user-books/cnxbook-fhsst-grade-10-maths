                    </m:mrow>
                  </m:math>
                </entry>
                <entry>1</entry>
                <entry>
                  <m:math overflow="scroll">
                    <m:mfrac>
                      <m:msqrt>
                        <m:mn>3</m:mn>
                      </m:msqrt>
                      <m:mn>2</m:mn>
                    </m:mfrac>
                  </m:math>
                </entry>
                <entry>
                  <m:math overflow="scroll">
