        <para id="id65008">
          <figure id="id65011">
            <media id="id65011_media" alt="">
              <image mime-type="image/png" src="../../media/MG10C14_010.png" id="id65011_onlineimage" width="356"><!-- NOTE: attribute width changes image size online (pixels). original width is 356. --></image>
