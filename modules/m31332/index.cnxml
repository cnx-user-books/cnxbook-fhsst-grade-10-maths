          <m:mfrac>
            <m:mrow>
              <m:mn>32</m:mn>
              <m:msup>
                <m:mi>a</m:mi>
                <m:mn>2</m:mn>
              </m:msup>
            </m:mrow>
            <m:mrow>
              <m:mn>4</m:mn>
