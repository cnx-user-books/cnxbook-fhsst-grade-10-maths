            <m:mo>∩</m:mo>
            <m:mi> ace </m:mi>
            <m:mo>)</m:mo>
          </m:mrow>
        </m:math>
      </equation>
      </item> <item> <label> Calculate the answer </label>
      <equation id="id114279">
        <m:math overflow="scroll" mode="display">
