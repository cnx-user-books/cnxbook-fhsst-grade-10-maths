    <entry><m:math><m:mi>Volume</m:mi><m:mo>=</m:mo><m:mi>l</m:mi><m:mo>×</m:mo><m:mi>b</m:mi><m:mo>×</m:mo><m:mi>h</m:mi></m:math></entry>
    <entry><m:math><m:mtable><m:mtr><m:mtd columnalign="right"><m:mi>Volume</m:mi></m:mtd><m:mtd><m:mo>=</m:mo></m:mtd><m:mtd columnalign="left"><m:mi>l</m:mi><m:mo>×</m:mo><m:mi>b</m:mi><m:mo>×</m:mo><m:mrow><m:mfrac><m:mn>1</m:mn><m:mn>2</m:mn></m:mfrac><m:mi>h</m:mi></m:mrow></m:mtd></m:mtr>
<m:mtr><m:mtd/><m:mtd><m:mo>=</m:mo></m:mtd><m:mtd columnalign="left"><m:mfrac><m:mn>1</m:mn><m:mn>2</m:mn></m:mfrac><m:mo>×</m:mo><m:mi>l</m:mi><m:mo>×</m:mo><m:mi>b</m:mi><m:mo>×</m:mo><m:mrow><m:mi>h</m:mi></m:mrow></m:mtd></m:mtr></m:mtable></m:math></entry>
